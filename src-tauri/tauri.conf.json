{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "i4RemoteCamerai",
  "version": "0.1.0",
  "identifier": "com.archwl.i4remotecamerai",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "i4RemoteCamerai",
        "width": 350,
        "height": 600,
        "resizable": false,
        "fullscreen": false,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}